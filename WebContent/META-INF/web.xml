<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>GlobalServelt</servlet-name>
		<servlet-class>com.apache.serlets.GlobalServelt</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GlobalServelt</servlet-name>
		<url-pattern>/globalServelt</url-pattern>
	</servlet-mapping>	
	
	<jsp-config>
		<jsp-property-group>
			<url-pattern>*.jsp</url-pattern>
			<include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
		</jsp-property-group>
	</jsp-config>
</web-app>